{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.24.0",
    "bootstrap": "^4.6.0",
    "glob-parent": "^6.0.1",
    "js-cookie": "^3.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.0",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.0.9",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.0",
    "remarkable": "^2.0.1",
    "typescript": "^4.3.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "watch": "npm-watch",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "watch": {
    "build": {
      "delay": 2500,
      "patterns": [
        "src",
        "public"
      ],
      "runOnChangeOnly": false,
      "legacyWatch": true,
      "extensions": "ts, tsx, css"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/user-event": "^14.0.3",
    "@types/jest": "^27.4.1",
    "@types/js-cookie": "^3.0.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.1.7",
    "@types/remarkable": "^2.0.3",
    "npm-watch": "^0.11.0"
  }
}
